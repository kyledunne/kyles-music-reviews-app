<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_music">

    <fragment
        android:id="@+id/nav_music"
        android:name="me.kyledunne.kylesmusicreviewsapp.ui.music.MusicFragment"
        android:label="@string/music"
        tools:layout="@layout/fragment_music" />

    <fragment
        android:id="@+id/nav_golf"
        android:name="me.kyledunne.kylesmusicreviewsapp.ui.golf.GolfFragment"
        android:label="@string/golf"
        tools:layout="@layout/fragment_golf" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="me.kyledunne.kylesmusicreviewsapp.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <action android:id="@+id/action_global_settingsFragment" app:destination="@id/settingsFragment" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="me.kyledunne.kylesmusicreviewsapp.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" /><action android:id="@+id/action_global_aboutFragment" app:destination="@id/aboutFragment"/>

</navigation>